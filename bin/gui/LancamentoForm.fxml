<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="TOP_CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.LancamentFormController">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <children>
      <Label text="Lancamento">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <HBox fillHeight="false" spacing="10.0">
         <children>
            <VBox maxWidth="117.0" prefHeight="62.0" prefWidth="35.0">
               <children>
                  <Label alignment="CENTER" text="ID">
                     <padding>
                        <Insets bottom="10.0" right="10.0" top="10.0" />
                     </padding>
                  </Label>
                  <TextField fx:id="txtId" alignment="CENTER" editable="false" opacity="0.4" prefHeight="26.0" prefWidth="73.0" />
               </children>
            </VBox>
            <VBox maxWidth="117.0" prefHeight="42.0" prefWidth="117.0">
               <children>
                  <Label alignment="CENTER" text="Nome">
                     <padding>
                        <Insets bottom="10.0" right="10.0" top="10.0" />
                     </padding>
                  </Label>
                  <TextField fx:id="txtNome" alignment="CENTER" prefHeight="0.0" prefWidth="117.0" text="aa" />
               </children>
            </VBox>
            <VBox maxWidth="117.0" prefHeight="42.0" prefWidth="117.0">
               <children>
                  <Label alignment="CENTER" text="Data">
                     <padding>
                        <Insets bottom="10.0" right="10.0" top="10.0" />
                     </padding>
                  </Label>
                  <TextField fx:id="txtData" alignment="CENTER" prefHeight="0.0" prefWidth="117.0" />
               </children>
            </VBox>
            <VBox maxWidth="117.0" prefHeight="42.0" prefWidth="117.0">
               <children>
                  <Label alignment="CENTER" text="Valor">
                     <padding>
                        <Insets bottom="10.0" right="10.0" top="10.0" />
                     </padding>
                  </Label>
                  <TextField fx:id="txtValor" alignment="CENTER" prefHeight="0.0" prefWidth="117.0" />
               </children>
            </VBox>
            <VBox maxWidth="117.0" prefHeight="42.0" prefWidth="117.0">
               <children>
                  <Label alignment="CENTER" text="Conta">
                     <padding>
                        <Insets bottom="10.0" right="10.0" top="10.0" />
                     </padding>
                  </Label>
                  <ComboBox fx:id="Conta" prefWidth="150.0" />
               </children>
            </VBox>
         </children>
      </HBox>
      <HBox alignment="TOP_RIGHT" fillHeight="false" spacing="10.0">
         <children>
            <Button fx:id="btnCancelar" mnemonicParsing="false" onAction="#btnCancelarAcao" text="Cancelar" />
            <Button fx:id="btnApagar" mnemonicParsing="false" onAction="#btnApagarAcao" text="Excluir" />
            <Button fx:id="btnSalvar" mnemonicParsing="false" onAction="#btnSalvarAcao" text="Salvar" />
         </children>
      </HBox>
   </children>
</VBox>
